{
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {

  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "all_frames": true,
      "run_at": "document_end",
      "js": [
        "overlay.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "overlay.js"
  ],
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "storage"
  ]
}
